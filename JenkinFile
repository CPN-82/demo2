pipeline{
    agent {
label 'master'
    }
    tools {
            maven 'm1'
          }
          stages {
 

  stage('build') {
    steps {
      sh 'mvn clean install'
    }
  }

  stage('test') {
    steps {
      sh 'mvn test'
    }
  }

  stage('post') {
    steps {
      archiveArtifacts artifacts: 'target/**.jar', followSymlinks: false
      
    }
  }

}

}
    
